clase X
	descripción "clase X"

	x1: entero := 1 : "Atributo x1";
	x2: entero := 2 : "Atributo x2";

	constructor(x1, x2:entero)
		descripcion ""
			entrada 
				x1: "Valor para x1";
				x2: "Valor para x2";
			pre { cierto }
		estrategia ""
	inicio
		este.x1 := x1;
		este.x2 := x2;
	fin constructor
fin clase

