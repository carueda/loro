}
{
	(
		e=e_const()
	  |
	  	e=e_este()
	  |
	    ti="["
		    /************* pendiente
		    [
			   LOOKAHEAD(2)         // for "[" "]"
			   t=ntipo()
			]
			************* pendiente */
		    e=expresion()     { list.add(e); }
		    ( 
			   LOOKAHEAD(2)         // for comma
			   "," e=expresion()  { list.add(e); } 
			)* 
			[ "," ]              
		tf="]"
		{
			e = ConstructorArbol.crearNExpresionArreglo(ti, list, tf);
		}
	  |
		e=e_nombre()
	  |
		e=e_qualified_nombre()
	  |
		"(" e=expresion() ")" 
	  |
		e=e_crear()
	)
	{
		return e;
	}
}

