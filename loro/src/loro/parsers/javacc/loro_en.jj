// Grammar for English
// $Id$

options {
	STATIC = false;
}

PARSER_BEGIN(LoroParser)
	package loro.parsers.javacc;
	import loro.arbol.*;
	import java.util.*;
	class LoroParser { }
PARSER_END(LoroParser)


SKIP : // spaces and comments
{
	" "
  | "\t"
  | "\n"
  | "\r"
  | "\f"
}

// Based on Java1.1.jj:

/* COMMENTS */

MORE :
{
  "//" : IN_SINGLE_LINE_COMMENT
|
  <"/**" ~["/"]> { input_stream.backup(1); } : IN_FORMAL_COMMENT
|
  "/*" : IN_MULTI_LINE_COMMENT
|
  "''" : IN_TEXT_DOC
|
  "{{" : IN_GUIDE
|
  "{%" : IN_IMPL
}

<IN_SINGLE_LINE_COMMENT>
SPECIAL_TOKEN :
{
  <SINGLE_LINE_COMMENT: "\n" | "\r" | "\r\n" > : DEFAULT
}

<IN_FORMAL_COMMENT>
SPECIAL_TOKEN :
{
  <FORMAL_COMMENT: "*/" > : DEFAULT
}

<IN_MULTI_LINE_COMMENT>
SPECIAL_TOKEN :
{
  <MULTI_LINE_COMMENT: "*/" > : DEFAULT
}


<IN_SINGLE_LINE_COMMENT,IN_FORMAL_COMMENT,IN_MULTI_LINE_COMMENT, IN_TEXT_DOC, IN_GUIDE, IN_IMPL>
MORE :
{
  < ~[] >
}

<IN_TEXT_DOC>
TOKEN :
{
  <TEXT_DOC: "''" > : DEFAULT
}

<IN_GUIDE>
TOKEN :
{
  <GUIDE: "}}" > : DEFAULT
}

<IN_IMPL>
TOKEN :
{
  <IMPL: "%}" > : DEFAULT
}


TOKEN : // basic types
{
	< BOOLEAN: "boolean" >
  | < STRING: "string" >
  | < CHAR: "char" >
  | < INT: "int" >
  | < REAL: "real" >
}

TOKEN : // other reserved words
{
	< ALGORITHM: "algorithm">
  | < CATCH: "catch">
  | < CASE: "case" >
  | < LOOP: "loop" >
  | < TRUE: "true" >
  | < CLASS: "class" >
  | < AS: "as" >
  | < CONSTANT: "constant">
  | < CONSTRUCTOR: "constructor">
  | < CONTINUE: "continue" >
  | < NEW: "new">
  | < FROM: "from" >
  | < TO: "to" >
  | < DESCRIPTION: "description">
  | < IN: "in">
  | < THEN: "then" >
  | < INPUT: "input">
  | < INSTANCEOF: "instanceof">
  | < SPECIFICATION: "specification">
  | < THIS: "this">
  | < EXISTS: "exists">
  | < EXTENDS: "extends">
  | < FALSE: "false" >
  | < END: "end" >
  | < GLOBAL: "global" >
  | < DO: "do">
  | < UNTIL: "until">
  | < IMPLEMENTATION: "implementation">
  | < BEGIN: "begin">
  | < IMPLEMENTS: "implements">
  | < TRY: "try">
  | < INTERFACE: "interface">
  | < THROW: "throw">
  | < METHOD: "method">
  | < WHILE: "while" >
  | < NULL: "null">
  | < OBJECT: "object">
  | < OPERATION: "operation">
  | < PACKAGE: "package">
  | < FOR: "for">
  | < DOWNTO: "downto">
  | < BY: "by" >
  | < FORALL: "forall">
  | < POST: "post">
  | < PRE: "pre">
  | < REPEAT: "repeat">
  | < RETURN: "return">
  | < OUTPUT: "output">
  | < SWITCH: "switch">
  | < IF: "if" >
  | < ELSE: "else" >
  | < ELSEIF: "elseif" >
  | < FINALLY: "finally" >
  | < SUPER: "super">
  | < BREAK: "break" >
  | < USES: "uses" >
}

TOKEN : // Literales
{
	< INT_LITERAL: (<DIGIT>)+ >
  |
	< REAL_LITERAL:
		(["0"-"9"])+ "." (["0"-"9"])+ (<EXPONENT>)?
	 |  "." (["0"-"9"])+ (<EXPONENT>)?
	 |  (["0"-"9"])+ <EXPONENT>
	 |  (["0"-"9"])+ (<EXPONENT>)?
	>
  |
	< #EXPONENT: ["e","E"] (["+","-"])? (["0"-"9"])+ >
  |
	< #HEXA: ["0"-"9","a"-"f","A"-"F"] >
  |
	< CHAR_LITERAL:
		  "'"
		  (   (~["'","\\","\n","\r"])
		| ("\\"
			( ["n","t","b","r","f","\\","'","\""]
			| ["0"-"7"] ( ["0"-"7"] )?
			| ["0"-"3"] ["0"-"7"] ["0"-"7"]
			| "u" <HEXA> <HEXA> <HEXA> <HEXA>
			)
		  )
		  )
		  "'"
	>
  |
	< STRING_LITERAL:   // may span several lines
	  "\""
	  (   (~["\"","\\"])    //(~["\"","\\","\n","\r"])
	| ("\\"
		( ["n","t","b","r","f","\\","'","\""]
		| ["0"-"7"] ( ["0"-"7"] )?
		| ["0"-"3"] ["0"-"7"] ["0"-"7"]
		| "u" <HEXA> <HEXA> <HEXA> <HEXA>
		)
	  )
	  )*
	  "\""
	>
}

TOKEN : // Identifiers
{
	< ID: <LETTER> (<LETTER>|<DIGIT>)* ("'")?>
  |
  < #LETTER:
	  [
	   "\u0024",
	   "\u0041"-"\u005a",
	   "\u005f",
	   "\u0061"-"\u007a",
	   "\u00c0"-"\u00d6",
	   "\u00d8"-"\u00f6",
	   "\u00f8"-"\u00ff",
	   "\u0100"-"\u1fff",
	   "\u3040"-"\u318f",
	   "\u3300"-"\u337f",
	   "\u3400"-"\u3d2d",
	   "\u4e00"-"\u9fff",
	   "\uf900"-"\ufaff"
	  ]
  >
|
  < #DIGIT:
	  [
	   "\u0030"-"\u0039",
	   "\u0660"-"\u0669",
	   "\u06f0"-"\u06f9",
	   "\u0966"-"\u096f",
	   "\u09e6"-"\u09ef",
	   "\u0a66"-"\u0a6f",
	   "\u0ae6"-"\u0aef",
	   "\u0b66"-"\u0b6f",
	   "\u0be7"-"\u0bef",
	   "\u0c66"-"\u0c6f",
	   "\u0ce6"-"\u0cef",
	   "\u0d66"-"\u0d6f",
	   "\u0e50"-"\u0e59",
	   "\u0ed0"-"\u0ed9",
	   "\u1040"-"\u1049"
	  ]
  >
}

