}
{
	ti=<FOR> [ "/" etq=tid() "/" ] ( LOOKAHEAD(2) d=declaracion() | i=tid() )
	(
		(<FROM> | ":=") ed=expresion()     
		( <TO>   |  <DOWNTO> { b = true; } ) eh=expresion()
		[ <BY> ep=expresion() ]
		<DO>
			a=acciones()
		tf=<END> [ LOOKAHEAD(2) tf=<FOR> ]
		{
