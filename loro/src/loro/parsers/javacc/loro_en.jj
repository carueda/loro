		}
	  |
		<IN> en=expresion()
		<DO>
			a=acciones()
		tf=<END> [ LOOKAHEAD(2) tf=<FOR> ]
		{
			return ConstructorArbol.crearNForEach(ti, etq, i, d, en, a, tf);
		}
	)
}

